{
  "name": "n8n-nodes-ubuntufabric",
  "version": "0.0.8",
  "description": "n8n community node to work with UbuntuFabric",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://ubuntufabric.io",
  "author": {
    "name": "UbuntuFabric",
    "email": "hello@UbuntuFabric.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UbuntuFabric/n8n-nodes-ubuntufabric.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/UbuntuFabricApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/UbuntuFabric/UbuntuFabric.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.82.0"
  }
}
